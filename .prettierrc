{
	"singleQuote": false,
	"trailingComma": "es5",
	"printWidth": 80,
	"tabWidth": 2,
	"semi": true,
	"proseWrap": "always",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"pluginSearchDirs": false,
	"overrides": [
	  {
		"files": "*.svelte",
		"options": {
		  "parser": "svelte"
		}
	  }
	]
  }
  